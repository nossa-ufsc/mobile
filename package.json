{
  "name": "nossa-ufsc",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "android": "expo run:android",
    "ios": "expo run:ios",
    "start": "expo start",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" && prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "web": "expo start --web",
    "dev": "bun run start",
    "build:ios:production": "eas build -p ios --profile production --auto-submit",
    "build:ios:development": "eas build -p ios --profile development",
    "build:android:production": "eas build -p android --profile production --local",
    "run:android": "expo prebuild && bun android",
    "run:ios": "expo prebuild && bun ios",
    "prepare": "husky"
  },
